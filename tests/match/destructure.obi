// Use parens because the match block
// declares the pattern and is evaluated once.
a :: (
  [5, _] -> isFive(),
  ["type" => "error", _] -> handleError(a),
  ["type" => "data", "data" => data] -> parseData(data),
)

// Use braces because it could be run more than once?
a :: {
  [5, _] -> isFive(),
  ["type" => "error", _] -> handleError(a),
  ["type" => "data", "data" => data] -> parseData(data),
}

